MEMORY
;   Name     Start     Size      Type
;   -------  --------  --------  ----
    EBOOT    00130000  00020000  RAMIMAGE
    RAM      00150000  00070000  RAM

    ETHDMA   00200000  00020000  RESERVED

; Total reserved area, which equals offset of the NK region in RAM
    dwReservedArea    00000000    00220000    FIXUPVAR
 
CONFIG
	COMPRESSION=OFF
	KERNELFIXUPS=ON

MODULES
;   Name            Path                                            Memory Type
;   --------------  ----------------------------------------------  -----------
    nk.exe          $(_PLATFORMROOT)\$(_TGTPLAT)\target\$(_TGTCPU)\$(WINCEDEBUG)\eboot.exe EBOOT
    
